# HealthVerse Backend Environment Configuration
# ===========================================

# Google Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_REASONING_MODEL=gemini-2.0-flash-exp
GEMINI_EMBEDDING_MODEL=text-embedding-004

# Qdrant Vector Database Configuration
QDRANT_CLUSTER_KEY=your_qdrant_cluster_key_here
QDRANT_CLUSTER_ID=your_qdrant_cluster_id_here
QDRANT_ENDPOINT=https://your-cluster-id.eu-west-2-0.aws.cloud.qdrant.io:6333
QDRANT_COLLECTION_NAME=healthverse_cases

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=False

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000","http://localhost:3001"]

# Medical Specialist Configuration
ALLOWED_DOCTORS=["Ophthalmologist","Optometrist","Optician","Ocular Surgeon"]

# Application Settings
LOG_LEVEL=INFO
MAX_QUESTION_COUNT=4
CONFIDENCE_THRESHOLD=0.85
